# Development Environment Configuration
APP_NAME=Enterprise Flutter App (Dev)
APP_VERSION=1.0.0-dev
ENVIRONMENT=development

# API Configuration
API_BASE_URL=https://dev-api.enterprise-app.com
AUTH_URL=https://dev-auth.enterprise-app.com
WEBSOCKET_URL=wss://dev-ws.enterprise-app.com

# Feature Flags
ENABLE_DEBUG_LOGGING=true
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=true
ENABLE_PERFORMANCE_MONITORING=true

# Security Settings
ENABLE_SSL_PINNING=false
ENABLE_ROOT_DETECTION=false
ENABLE_JAILBREAK_DETECTION=false

# Third-party Services
FIREBASE_PROJECT_ID=enterprise-app-dev
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id-dev

# Build Configuration
OBFUSCATE_CODE=false
GENERATE_SOURCE_MAPS=true
MINIFY_CODE=false