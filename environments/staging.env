# Staging Environment Configuration
APP_NAME=Enterprise Flutter App (Staging)
APP_VERSION=1.0.0-rc
ENVIRONMENT=staging

# API Configuration
API_BASE_URL=https://staging-api.enterprise-app.com
AUTH_URL=https://staging-auth.enterprise-app.com
WEBSOCKET_URL=wss://staging-ws.enterprise-app.com

# Feature Flags
ENABLE_DEBUG_LOGGING=false
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_PERFORMANCE_MONITORING=true

# Security Settings
ENABLE_SSL_PINNING=true
ENABLE_ROOT_DETECTION=true
ENABLE_JAILBREAK_DETECTION=true

# Third-party Services
FIREBASE_PROJECT_ID=enterprise-app-staging
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id-staging

# Build Configuration
OBFUSCATE_CODE=true
GENERATE_SOURCE_MAPS=true
MINIFY_CODE=true